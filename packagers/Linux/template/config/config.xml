<?xml version="1.0" encoding="UTF-8"?>
<Installer>
	<Name>Sacada</Name>
	<Version>1.0</Version>
	<Title>Sacada Installer</Title>
	<Publisher>Jalopy Software</Publisher>
	<ProductUrl>https://github.com/Jalopy-Tech/Sacada/blob/master/README.md</ProductUrl>
	<TargetDir>@HomeDir@/sacada</TargetDir>
	<RunProgram>@TargetDir@/sacada.sh</RunProgram>
	<RunProgramDescription>Launch Sacada</RunProgramDescription>
	<MaintenanceToolName>sacada-maintenance-tool</MaintenanceToolName>
	<MaintenanceToolIniFile>sacada-maintenance-tool</MaintenanceToolIniFile>
	<WizardDefaultWidth>800</WizardDefaultWidth>
	<WizardStyle>Modern</WizardStyle>
	<Logo>logo.png</Logo>
	<Watermark>watermark.png</Watermark>
	<ControlScript>controlscript.js</ControlScript>
	<SupportsModify>false</SupportsModify>
	<CreateLocalRepository>false</CreateLocalRepository>
</Installer>
